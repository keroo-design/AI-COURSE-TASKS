#shallow copy
l1=[1,22,34,[30,40,50,[90,100,400]]]
l2=l1.copy()
l1[3].append(100)
print(l1)
print(l2)

answer is:   [1, 22, 34, [30, 40, 50, [90, 100, 400], 100]]
             [1, 22, 34, [30, 40, 50, [90, 100, 400], 100]]




#deep copy

import copy
l1 = [1, 22, 34, [30, 40, 50, [90, 100, 400]]]
l2 = copy.deepcopy(l1)

l1[3].append(100)

print(l1)
print(l2)
   
  answer is:       [1, 22, 34, [30, 40, 50, [90, 100, 400], 100]]
                   [1, 22, 34, [30, 40, 50, [90, 100, 400]]]
